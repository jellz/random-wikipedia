{
  "name": "Random Wikipedia",
  "version": "1.0",
  "description": "Wikipedia randomness",
  "permissions": ["declarativeContent", "storage", "tabs", "alarms"],
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "page_action": {
    "default_popup": "popup.html"
  }
}